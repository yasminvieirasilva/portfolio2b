void setup()
{
  pinMode(4, OUTPUT);
}

void loop()
{
  digitalWrite(4, HIGH);
  delay(1000); 
  digitalWrite(4, LOW);
  delay(100); 
   digitalWrite(4, HIGH);
  delay(1000); 
  digitalWrite(4, LOW);
  digitalWrite(4, HIGH);
  delay(1000); 
  digitalWrite(4, LOW);
  delay(100); 
  delay(1000); 
  digitalWrite(4, LOW);
  delay(100); 
   digitalWrite(5, HIGH);
  delay(1000); 
  digitalWrite(5, LOW);
  delay(100); 
     digitalWrite(2, HIGH);
  delay(1000);
  digitalWrite(2, LOW);
  delay(100); 
   digitalWrite(5, HIGH);
  delay(1000); 
  digitalWrite(5, LOW);
  delay(100); 
   digitalWrite(5, HIGH);
  delay(1000); 
  digitalWrite(5, LOW);
  delay(100); 
  digitalWrite(2, HIGH);
  delay(1000); 
  digitalWrite(2, LOW);
  delay(100); 
 
}
